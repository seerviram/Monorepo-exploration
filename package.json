{
  "name": "fe-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "npm@10.9.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:ports": "concurrently -n app-one,app-two -c auto \"npm run -w @acme/app-one dev -- --port 5173\" \"npm run -w @acme/app-two dev -- --port 5174\"",
    "build": "turbo run build",
    "preview": "turbo run preview --parallel",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "clean": "turbo run clean && rimraf node_modules"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "rimraf": "^6.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.6.0",
    "prettier": "^3.3.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0"
  }
}